@model List<Student>
<!--Strong Type View-->

<a asp-action="Create" asp-controller="Student" class="btn btn-primary mb-3">Add new Student</a>
<table class="table table-dark table-striped text-center" width="50%">
    <tr>
        <th>
            Name
        </th>
        <th>
            Age
        </th>
        <th>
            Address
        </th>
        <th>
            Track
        </th>
        <th>
            Instructor
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>@item.Address</td>
            <td>@item.Track</td>
            <td>@item.Instructor</td>
        </tr>
    }
</table>